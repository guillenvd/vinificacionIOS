<?xml version='1.0' encoding='utf-8'?>
<widget id="com.maxicomm.vinificacion" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <allow-intent href="market:*" />
    <icon src="www/img/icon.png" />
    <icon density="ldpi" src="www/img/icon.png" width="150" />
    <icon density="mdpi" src="www/img/icon.png" width="150" />
    <icon density="hdpi" src="www/img/icon.png" width="150" />
    <icon density="xhdpi" src="www/img/icon.png" width="150" />
    <name>Vinificacion</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
